{
    "manifest_version": 3,
    "name": "XTXP",
    "version": "1.0",
    "description": "Export and import Chrome extensions",
    "permissions": ["management"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon.png",
        "48": "icons/icon.png",
        "128": "icons/icon.png"
      }
    },
    "icons": {
      "16": "icons/icon.png",
      "48": "icons/icon.png",
      "128": "icons/icon.png"
    }
  }
  